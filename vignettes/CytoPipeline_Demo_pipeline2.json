{
  "scaleTransformProcessingSteps": [  	
	{
	  "name": ["scale_transform_read"],
      "FUN": ["readRDSObject"],
      "ARGS": { 
	    "RDSFile": ["CytoPipelineDemo_scale_transfos.rds"]
	  }
	}
  ],  
  "flowFramesPreProcessingSteps": [
    {
      "name": ["flowframe_read"],
      "FUN": ["readSampleFiles"],
      "ARGS": {
        "truncate_max_range": [false],
        "min.limit": null
      }
    }, 
    {
      "name": ["remove_margins"],
      "FUN": ["removeMarginsPeacoQC"],
      "ARGS": {
        "channelSpecifications": {
          "AllFluoChannels": [-300, 262144]
        }
      }
    },	
	{
      "name": ["remove_doublets"],
      "FUN": ["removeDoubletsCytoPipeline"],
      "ARGS": {
        "areaChannels": ["FSC-A"],
        "heightChannels": ["FSC-H"],
        "nmads": [3]
      }
    },
    {
      "name": ["remove_debris"],
      "FUN": ["removeDebrisFlowClustTmix"],
      "ARGS": {
        "FSCChannel": ["FSC-A"],
        "SSCChannel": ["SSC-A"],
        "nClust": [3],
        "level": [0.97],
		"B": [100]}
    },
	{
      "name": ["compensate"],
      "FUN": ["compensateFromMatrix"],
      "ARGS": {
        "matrixSource": ["import"],
		"matrixPath" : ["Compensations Liver D91.csv"]
      }
    },
	{
      "name": ["perform_QC"],
      "FUN": ["qualityControlPeacoQC"],
      "ARGS": {
        "preTransform": [true],
        "min_cells": [150],
        "max_bins": [500],
        "step": [500],
        "MAD": [6],
        "IT_limit": [0.55],
        "force_IT": [150],
        "peak_removal": [0.3333],
        "min_nr_bins_peakdetection": [10]
      }
    }   
  ]  
} 
