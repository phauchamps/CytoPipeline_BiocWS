{
  "scaleTransformProcessingSteps": [  	
	{
	  "name": ["scale_transform_read"],
      "FUN": ["readRDSObject"],
      "ARGS": { 
	    "RDSFile": ["CytoPipelineDemo_scale_transfos.rds"]
	  }
	}
  ],  
  "flowFramesPreProcessingSteps": [
    {
      "name": ["flowframe_read"],
      "FUN": ["readSampleFiles"],
      "ARGS": {
        "truncate_max_range": [false],
        "min.limit": null
      }
    }, 
    {
      "name": ["remove_margins"],
      "FUN": ["removeMarginsPeacoQC"],
      "ARGS": {
        "channelSpecifications": {
          "AllFluoChannels": [-300, 262144]
        }
      }
    },	
	{
      "name": ["perform_QC"],
      "FUN": ["qualityControlFlowAI"],
      "ARGS": {		  
        "preTransform": [false],
        "remove_from": ["all"],
        "second_fractionFR": [0.1],
        "deviationFR": ["MAD"],
        "alphaFR": [0.01],
        "decompFR": [true],
        "outlier_binsFS": [false],
        "pen_valueFS": [200],
        "max_cptFS": [3],
		"sideFM": ["both"],
		"neg_valuesFM": [1]
      }
    },
	{
      "name": ["remove_doublets"],
      "FUN": ["removeDoubletsCytoPipeline"],
      "ARGS": {
        "areaChannels": ["FSC-A"],
        "heightChannels": ["FSC-H"],
        "nmads": [3]
      }
    },
    {
      "name": ["remove_debris"],
      "FUN": ["removeDebrisFlowClustTmix"],
      "ARGS": {
        "FSCChannel": ["FSC-A"],
        "SSCChannel": ["SSC-A"],
        "nClust": [2],
        "level": [0.97],
		"B": [100]}
    },
	{
      "name": ["compensate"],
      "FUN": ["compensateFromMatrix"],
      "ARGS": {
        "matrixSource": ["import"],
		"matrixPath" : ["Compensations Liver D91.csv"]
      }
    }
  ]  
}
